  "type": "module",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "node --watch src/index.js",
    "test": "node --test",
    "test:agent": "node scripts/test-seo-agent.js",
    "test:db": "node scripts/test-database.js",
    "lint": "echo 'ESLint not configured yet - see Phase 1.4.2' && exit 0",
